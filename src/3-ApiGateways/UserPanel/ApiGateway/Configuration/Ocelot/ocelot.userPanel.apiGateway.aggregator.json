{
  // uaoa => userPanel api gateway owners aggregator
  "Routes": [
    {
      "SwaggerKey": "UserPanel.ApiGateway.Aggregator",
      "Priority": 0,
      "UpstreamPathTemplate": "/uaoa/{everything}",
      "UpstreamHttpMethod": [ "GET" ],
      "DownstreamPathTemplate": "/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5003
        }
      ],
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "IdentityApiKey",
        "AllowedScopes": [ "userPanel.apiGateway.aggregator.read", "profile", "openid" ]
      }

    }
  ]
}